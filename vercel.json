{
  "version": 2,
  "routes": [
    {
      "src": "/offer1(.*)",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-ip-country",
          "value": "TZ"
        }
      ],
      "dest": "https://kutombana.vercel.app/$1",
      "status": 307
    },
    {
      "src": "/offer2(.*)",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-ip-country",
          "value": "TZ"
        }
      ],
      "dest": "https://kutombana.vercel.app/$1",
      "status": 307
    },
    {
      "src": "/special(.*)",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-ip-country",
          "value": "TZ"
        }
      ],
      "dest": "https://wakora.site/$1",
      "status": 307
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
